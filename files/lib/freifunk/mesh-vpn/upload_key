#!/bin/sh
	## Key-Upload
        URL_1='http://fastd1.kbu.freifunk.net'
        URL_2='http://fastd2.kbu.freifunk.net'
        URL_3='http://fastd3.kbu.freifunk.net'
        URL_4='http://fastd4.kbu.freifunk.net'

        /lib/freifunk/mesh-vpn/upload_key_to_server $URL_1 &
        /lib/freifunk/mesh-vpn/upload_key_to_server $URL_2 &
        /lib/freifunk/mesh-vpn/upload_key_to_server $URL_3 &
        /lib/freifunk/mesh-vpn/upload_key_to_server $URL_4 &

