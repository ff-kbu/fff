        URL_3='http://vpn3.kbu.freifunk.net:8081/fastd-upload/'
        URL_4='http://vpn4.kbu.freifunk.net:8081/fastd-upload/'
        URL_5='http://vpn5.kbu.freifunk.net:8081/fastd-upload/'
        URL_6='http://vpn6.kbu.freifunk.net:8081/fastd-upload/'
        URL_7='http://vpn7.kbu.freifunk.net:8081/fastd-upload/'
        URL_8='http://vpn8.kbu.freifunk.net:8081/fastd-upload/'
